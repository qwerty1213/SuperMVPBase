<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:descendantFocusability="blocksDescendants">

    <LinearLayout
        android:id="@+id/container"
        style="@style/MatchMatch.Vertical"
        android:descendantFocusability="blocksDescendants">

        <include layout="@layout/imclude_ondemand_details_top_image" />

        <LinearLayout
            style="@style/MatchMatch.Vertical"
            android:paddingBottom="20dp"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:paddingTop="20dp">

            <TextView
                android:id="@+id/txt_headliness"
                style="@style/text_black17_wrap"
                android:text="ssssssss"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/txt_period_validity"
                style="@style/text_c33_15_wrap"
                android:layout_marginTop="15dp" />

            <TextView
                android:id="@+id/txt_note"
                style="@style/text_c99_14_wrap"
                android:layout_marginTop="13dp" />

        </LinearLayout>

        <include layout="@layout/include_lines_5_f7f7" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:gravity="center_vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:text="@string/text_details_introduced"
            android:textColor="@color/black"
            android:textSize="14sp" />

        <include layout="@layout/include_lines_f7f7" />

    </LinearLayout>
</android.support.v4.widget.NestedScrollView>